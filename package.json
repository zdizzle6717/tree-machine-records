    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.24.1",
    "sequelize-cli": "^2.4.0",
    "vision": "^4.1.0",
