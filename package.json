{
  "name": "tree-machine-records",
  "version": "2.0.0",
  "description": "This is a universal javascript app built with react on the server side and client site demonstrated with simple movie data",
  "license": "MIT",
  "main": "index.js",
	"repository": {
    "type": "git",
    "url": "git+https://github.com/zdizzle6717/tree-machine-records.git"
  },
  "keywords": [
    "javascript",
    "universal",
    "react",
    "starter",
    "kit",
    "app",
    "reactjs",
    "router",
    "express",
    "hapijs"
  ],
  "author": "Zack Anselm",
  "bugs": {
    "url": "https://github.com/zdizzle6717/tree-machine-records/issues"
  },
	"scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-client": "babel-watch server-client/index.jsx",
    "compile-client": "NODE_ENV=production node_modules/.bin/webpack -p --config webpack.client.js",
    "start-client": "npm run compile-client && pm2 start server-client/compiled.js --name='TMRecords'",
    "stop-client": "pm2 stop 'TMRecords'",
    "watch-server": "babel-watch server/index.js",
    "compile-server": "babel server --out-dir compiled-server",
    "start-server": "npm run compile-server && pm2 start compiled-server/index.js --name='TMRecordsAPI'",
    "stop-server": "pm2 stop 'TMRecordsAPI'",
    "style": "node build-style.js",
    "build": "node build-style.js && NODE_ENV=development node_modules/.bin/webpack -d --config webpack.dev.js",
    "build-prod": "node build-style.js && NODE_ENV=production node_modules/.bin/webpack -p --config webpack.prod.js",
    "deploy": "npm run build-prod && npm run start-server && npm run start-client"
  },
  "dependencies": {
    "attr-accept": "^1.1.0",
